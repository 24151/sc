00  LOAD 30      ; Счетчик i = 1
01  STORE 31     ; i -> cell 31
02  LOAD 40      ; acc = 0 (накопитель суммы)
03  STORE 32     ; sum = 0
04  LOAD 31      ; acc = i
05  MUL 31       ; i * i
06  ADD 32       ; acc += sum
07  STORE 32     ; sum = acc
08  LOAD 31
09  ADD 33       ; i = i + 1
10  STORE 31
11  LOAD 31
12  SUB 34       ; if i - 6 < 0 → continue
13  JNEG 04      ; loop

14  WRITE 32     ; вывод суммы квадратов
15  HALT 00

; --- Данные ---
30 = +0001       ; начальное значение i = 1
31 = +0000       ; i (текущее)
32 = +0000       ; sum (накопитель)
33 = +0001       ; константа +1
34 = +0006       ; предел цикла (6 → i < 6)
